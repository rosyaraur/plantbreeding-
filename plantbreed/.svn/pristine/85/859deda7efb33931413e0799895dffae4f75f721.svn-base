\seealso{
%% ~~objects to See Also as \code{\link{help}}, ~~~
}
\examples{
data(linetester)
pls <- line.tester(dataframe = linetester, yvar = "trait1",  genotypes = "genotypes", 
replication = "replication",  Lines = "Lines", Testers = "Tester", gclass = "gclass" ) 
print(pls) 
}
